    .scope S_0x5652fed950d0;
T_2 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5652fedddf20_0, 0, 32;
T_2.0 ;
    %load/vec4 v0x5652fedddf20_0;
    %cmpi/s 2, 0, 32;
    %jmp/0xz T_2.1, 5;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5652fedde010_0, 0, 32;
T_2.2 ;
    %load/vec4 v0x5652fedde010_0;
    %cmpi/s 2, 0, 32;
    %jmp/0xz T_2.3, 5;
    %load/vec4 v0x5652fedddf20_0;
    %pad/s 34;
    %pad/s 35;
    %muli 2, 0, 35;
    %pad/s 36;
    %load/vec4 v0x5652fedde010_0;
    %pad/s 36;
